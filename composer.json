{
  "name": "imsidz/laravel-friendships",
  "description": "This package gives Eloquent models the ability to manage their friendships.",
  "keywords": [
    "laravel",
    "friendships",
    "friend-system",
    "friends",
    "eloquent"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Imsidz",
      "email": "imsidzluv@gmail.com"
    },
    {
      "name": "Imsidz",
      "email": "Imsidz@icloud.com"
    }
  ],
  "autoload-dev": {
    "classmap": [
      "vendor/laravel/laravel/tests/TestCase.php"
    ],
    "files": [
      "tests/helpers.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "fzaninotto/faker": "~1.4",
    "laravel/laravel": "6.*",
    "codeclimate/php-test-reporter": "^0.3.2",
    "mockery/mockery": "^0.9.5",
    "doctrine/dbal": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "imsidz\\Friendships\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "imsidz\\Friendships\\FriendshipsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
